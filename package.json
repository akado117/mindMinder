{
  "name": "cuminu",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "yarn eslint 'src/**/*.{ts,tsx}'",
    "format": "yarn prettier --write 'src/**/*.{ts,tsx}'",
    "format:check": "yarn prettier --check 'src/**/*.{ts,tsx}'",
    "typecheck": "yarn tsc --noEmit --project tsconfig.json",
    "typecheck:watch": "yarn tsc --watch --noEmit --project tsconfig.json"
  },
  "dependencies": {
    "@material-ui/core": "4.12.2",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@material-ui/styles": "4.11.4",
    "@reduxjs/toolkit": "1.6.1",
    "@sentry/browser": "6.9.0",
    "date-fns": "^2.22.1",
    "email-validator": "^2.0.4",
    "firebase": "^8.8.0",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-calendar": "^3.4.0",
    "react-dom": "17.0.2",
    "react-ga": "3.3.0",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "@vitejs/plugin-react-refresh": "1.3.1",
    "autoprefixer": "10.2.5",
    "csx": "^10.0.2",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.22.0",
    "normalize.css": "^8.0.1",
    "postcss": "8.2.10",
    "prettier": "2.2.1",
    "tailwindcss": "2.1.1",
    "typescript": "4.3.2",
    "vite": "2.4.3"
  }
}
